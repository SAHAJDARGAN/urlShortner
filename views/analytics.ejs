<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  <link rel="stylesheet" href="/stylesheets/profile.css">
    <!-- <link rel="stylesheet" href="/stylesheets/feed.css"> -->
</head>
<body>


    <br><br><br><br><br><br>
    <style>
        .table-cell {
          padding: 40px;
          /* Adjust the padding value to your preference */
        }
      </style>
      <hr>
      <div class="container">
        <table>
          <thead>
            <tr>
              <th class="table-cell">Original URL</th>
              <!-- <th class="table-cell">Short ID</th> -->
              <th class="table-cell">Analytics(No. of Clicks)</th>
            </tr>
          </thead>
          <tbody>
            <% user.Urls.forEach(function(url) { %>
              <tr>
                <td class="table-cell">
                  <%= url.redirectURL %>
                </td>
                <!-- <td class="table-cell"><a href="/redirect/<%= url.shortId %>">
                    <%= url.shortId %>
                  </a> -->
                </td>
                <td class="table-cell" id="analytics-<%= url.shortId %>">
                  <%=url.visitHistory.length%>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
        <hr>
      </div>
</body>
                 

</html>